{
  "server": {
    "http_port": 8082,
    "grpc_port": 50052,
    "environment": "development"
  },
  "log": {
    "level": "info",
    "filename": "./logs/app.log",
    "maxsize": 100,
    "maxbackups": 4,
    "maxage": 7,
    "compress": true,
    "console": true
  },
  "redis": {
    "addr": "127.0.0.1:6379",
    "password": "",
    "db": 0,
    "pool_size": 10
  },
  "database": {
    "dsn": "host=localhost port=5432 user=postgres password=xxx dbname=testdb sslmode=disable",
    "log_level": "info",
    "slow_threshold": "1s",
    "dry_run": false,
    "max_open_conns": 100,
    "max_idle_conns": 10,
    "conn_max_lifetime": "30m"
  }
}